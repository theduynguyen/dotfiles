{
  "vim.disableAnnoyingNeovimMessage": true,
  "python.jediEnabled": false,
  "cmake.configureOnOpen": true,
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4 }",
  "workbench.colorTheme": "Atom One Dark",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "editor.formatOnType": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "[javascript]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "cmake.revealLog": "focus",
  "cmake.installPrefix": "${workspaceFolder}/build/install",
  "cmake.generator": "Ninja",
  "cmake.preferredGenerators": ["Ninja", "Unix Makefiles"],
  "cmake.configureSettings": {
    "VCPKG_TARGET_TRIPLET": "x64-osx",
    "CMAKE_TOOLCHAIN_FILE": "/Users/tdnguyen/SW/vcpkg/scripts/buildsystems/vcpkg.cmake"
  },
  "prettier.semi": false,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "files.associations": {
    "*.env": "shellscript"
  },
  "diffEditor.ignoreTrimWhitespace": true,
  "csv-preview.separator": ";",
  "python.showStartPage": false,
  "git.confirmSync": false,
  "explorer.autoReveal": "focusNoScroll",
  "window.zoomLevel": 0,
  "workbench.startupEditor": "newUntitledFile",
  "python.languageServer": "Pylance",
  "auto-close-tag.SublimeText3Mode": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.foldfix": true,
  "vim.autoindent": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["o", "o"],
      "after": ["o", "<Esc>", "k"]
    },
    {
      "before": ["O", "O"],
      "after": ["O", "<Esc>", "j"]
    },
    {
      "before": ["<Space>"],
      "commands": ["editor.toggleFold"]
    },
    {
      "before": ["<Leader>","<Leader>","w"],
      "after": ["<Leader>","<Leader>","<Leader>","b","d","w"]
    },
    {
      "before": ["<Leader>","<Leader>","e"],
      "after": ["<Leader>","<Leader>","<Leader>","b","d","e"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["2", "0", "j"]
    },
    {
      "before": ["K"],
      "after": ["2", "0", "k"]
    },
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["g", "c"],
      "commands": ["editor.action.commentLine"],
      "when": "editorTextFocus && !editorReadonly"
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.smartRelativeLine": true,
  "editor.formatOnPaste": true
}
